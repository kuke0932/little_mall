/* pages/pushOut/pushOut.wxss */
.moneyDetal {
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-template-rows: 80rpx 80rpx;
  line-height: 80rpx; 
  border-bottom: 1px solid #eee;
}
.moneyDetal view {
  text-align: center;
  color: #666;
  font-size: 30rpx;
}
.moneyDetal .getCash{
  margin: 10rpx auto;
  width: 60%;
  height: 60rpx;
  line-height: 60rpx;
  background: #47B34F;
  color: #fff;
}
text {
  color: #666;
}
.rule {
  text-align: center;
  margin-top: 100rpx;
}
.rule .t {
  font-size: 48rpx;
}
.ruleDetails {
  padding: 30rpx;
}
.ruleDetails .v {
  margin-bottom: 20rpx;
  text-align: justify;
}
.ruleDetails .t {
  font-size: 32rpx;
}
.btnBox {
  width: 100%;
  display: flex;
  position: fixed;
  bottom: 0;
  height: 100rpx;
  line-height: 100rpx;
}
.btnBox .left {
  flex: 1.5;
  border-top: 1px solid #eee;
  box-sizing: border-box;
  text-align: center;
}
.btnBox .right {
  flex: 2;
  text-align: center;
  background: #47B34F;
}
.qrcodePop {
  width: 100%;
  height: 100%;
  position: fixed;
  background: rgba(0, 0, 0, 0.5);
  z-index: 8;
  bottom: 0;
}
.qrcodePop .imgBox {
  display: block;
  position: fixed;
  z-index: 9;
  top:50%;
  left: 50%;
  margin-top: -250rpx;
  margin-left: -250rpx;
}
.qrcodePop .c {
  width: 100%;
  text-align: center;
  color: #eee;
  margin-top: 30rpx;
}
.qrcodePop .t{
  color: darkgrey;
  font-size: 34rpx;
}
.cashModel {
  width: 100%;
  height: 100%;
  position: fixed;
  background: rgba(0, 0, 0, 0.5);
  z-index: 8;
  bottom: 0;
}
.cashModel .modelBox{
  position: relative;
  width: 85%;
  margin: 250rpx auto;
  min-height: 500rpx;
  background: #fff;
}
.cashModel .modelBox .t{
  position: absolute;
  top: 40rpx;
  left: 40rpx;
  font-size: 40rpx;
  color: #111;
}
.cashModel .modelBox .notice{
  position: absolute;
  bottom: 170rpx;
  left: 40rpx;
  font-size: 28rpx;
  color: #999;
}
.cashModel .modelBox .box {
  position: absolute;
  display: flex;
  top: 30%;
  height: 120rpx;
  border-bottom: 1px solid #ccc;
  margin: 0 50rpx;
}
.cashModel .modelBox .box .y{
  flex:1;
  margin-right: 20rpx;
  font-size: 100rpx;
  font-weight: bold;
  color: #000;
  height: 120rpx;
}
.cashModel .modelBox .box .i {
  flex: 8;
  height: 100rpx;
  font-size: 100rpx;
  font-weight: bold;
  color: #000;
}
.cashModel .modelBox .confirm_cancel{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  display: grid;
  grid-template-columns: 1fr 1.5fr;
  grid-template-rows: 100rpx 100rpx;
  line-height: 100rpx;
  text-align: center;
}
.cashModel .modelBox .confirm_cancel .left{
  border-top: 1px solid #eee;
  color: #111;
}
.cashModel .modelBox .confirm_cancel .right{
  background: #47B34F;
  color: #fff;
}